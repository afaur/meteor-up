{
  "name": "mup",
  "version": "1.0.0",
  "description": "Production Quality Meteor Deployments",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "bin": {
    "mup": "./index.js"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./lib",
    "test": "./node_modules/.bin/mocha $npm_package_options_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadirahq/meteor-up.git"
  },
  "keywords": [
    "meteor"
  ],
  "author": "Kadira Inc.",
  "license": "MIT",
  "options": {
    "mocha": "--no-colors --require scripts/mocha-bootload lib/**/__tests__/**/*.js"
  },
  "bugs": {
    "url": "https://github.com/kadirahq/meteor-up/issues"
  },
  "homepage": "https://github.com/kadirahq/meteor-up#readme",
  "devDependencies": {
    "babel-eslint": "4.x.x",
    "eslint": "1.x.x",
    "eslint-plugin-babel": "2.x.x",
    "mocha": "2.x.x",
    "ssh2": "0.4.x"
  },
  "dependencies": {
    "archiver": "0.20.x",
    "babel-core": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-0": "6.x.x",
    "bluebird": "3.x.x",
    "debug": "2.x.x",
    "nodemiral": "1.x.x",
    "shelljs": "0.5.x",
    "silent-npm-registry-client": "1.x.x",
    "underscore": "1.x.x",
    "uuid": "2.x.x"
  }
}
